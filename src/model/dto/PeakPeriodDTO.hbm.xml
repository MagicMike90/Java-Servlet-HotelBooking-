<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="model.dto.PeakPeriodDTO" table="peak_periods">
		<meta attribute="class-description">
			This class contains the details for premium percentage
		</meta>
		<id name="id" type="int" column="peak_period_id">
			<generator class="native"></generator>
		</id>
		<property name="startDate" column="start_date" type="calendar_date" />
		<property name="endDate" column="end_date" type="calendar_date" />
		<property name="premiumPercentage" column="premium_percentage" type="int" />
	</class>
</hibernate-mapping>
